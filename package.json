  ],
  "repository": "generate/generate-robots",
  "bugs": {
    "url": "https://github.com/generate/generate-robots/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-valid-app": "^0.2.0"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "generate": "^0.11.2",
